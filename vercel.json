{
    "version": 2,
    "buildCommand": "cd packages/web && npm install && npm run build",
    "outputDirectory": "packages/web/dist",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}