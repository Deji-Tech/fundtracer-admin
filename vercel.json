{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "packages/web/dist",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api"
        }
    ],
    "functions": {
        "api/index.ts": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}